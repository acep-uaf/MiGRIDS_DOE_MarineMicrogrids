<!-- This file describes the inputs for the class 'predictWind0'. -->
<!-- Root tag is 'tesDispatchName' -->
<predictWindName name = 'predictWind2'>
    <!-- 'std' is the standard deviation in wind predicions, normalized to the true value of future wind.
    Default is 0.25. -->
    <std value = '0.25'/>
    <!-- 'nearTermWindow' is the amount of time that the near term low wind prediction will be for, for spinning reserve
    purposes. Default is 300 s (5 min). -->
    <nearTermWindow value = '300'/>
</predictWindName>
